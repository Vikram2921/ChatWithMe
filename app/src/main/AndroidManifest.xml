<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nobodyknows.chatwithme">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChatWithMe">
        <activity android:name=".Activities.ChatRoom">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Dashboard.AddNewChat" />
        <activity android:name=".Activities.LoginContinue" />
        <activity android:name=".Activities.Dashboard.Dashboard" />
        <activity android:name=".Activities.Signup.MobileVerification" />
        <activity android:name=".Activities.Signup.CreateUser" />
        <activity android:name=".Activities.Login" />
        <activity android:name=".MainActivity">
        </activity>
    </application>

</manifest>